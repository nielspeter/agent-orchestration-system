{
  "model": "claude-3-5-haiku-latest",
  "mcpServers": {
    "docling": {
      "command": "uvx",
      "args": [
        "--from=docling-mcp",
        "docling-mcp-server"
      ],
      "description": "Docling document processing and conversion server"
    }
  },
  "agents": {
    "directories": ["examples/07-mcp-docling/agents"]
  },
  "tools": {
    "builtin": ["read", "write", "list"]
  },
  "safety": {
    "maxIterations": 15,
    "warnAtIteration": 10,
    "maxTokensEstimate": 50000,
    "maxDepth": 3
  }
}
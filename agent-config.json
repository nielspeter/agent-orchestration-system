{
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time",
        "--local-timezone=America/New_York"
      ],
      "description": "Time and timezone utilities"
    }
  },
  "builtinTools": {
    "enabled": true,
    "tools": ["read", "write", "list", "task", "todowrite"]
  },
  "agents": {
    "directory": "./agents",
    "additionalDirectories": [
      "./examples/agents"
    ]
  },
  "execution": {
    "defaultModel": "claude-3-5-haiku-20241022",
    "maxDepth": 10,
    "maxIterations": 100,
    "timeout": 300000
  }
}
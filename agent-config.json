{
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=America/New_York"],
      "description": "Time and timezone utilities"
    }
  },
  "builtinTools": {
    "enabled": true,
    "tools": ["read", "write", "list", "task", "todowrite"]
  },
  "agents": {
    "directory": "./agents",
    "additionalDirectories": []
  },
  "execution": {
    "defaultModel": "anthropic/claude-3-5-haiku-latest",
    "defaultBehavior": "balanced",
    "maxDepth": 10,
    "maxIterations": 100,
    "timeout": 300000
  },
  "storage": {
    "type": "noop",
    "options": {
      "path": ".agent-sessions"
    }
  }
}

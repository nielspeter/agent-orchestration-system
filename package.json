{
  "name": "agent-orchestration-system",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Agent orchestration system where everything is an agent",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielspeter/agent-orchestration-system.git"
  },
  "bugs": {
    "url": "https://github.com/nielspeter/agent-orchestration-system/issues"
  },
  "homepage": "https://github.com/nielspeter/agent-orchestration-system#readme",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w @agent-system/core",
    "build:web": "npm run build -w @agent-system/web",
    "dev": "npm run dev -w @agent-system/core",
    "dev:web": "npm run dev -w @agent-system/web",
    "test": "npm run test -w @agent-system/core",
    "test:unit": "npm run test -w @agent-system/core",
    "test:unit:coverage": "npm run test:coverage -w @agent-system/core",
    "test:integration": "vitest run --config packages/core/vitest.config.integration.ts",
    "test:integration:werewolf": "vitest run --config packages/core/vitest.config.integration.ts packages/core/tests/integration/werewolf-game/",
    "test:integration:critical-illness": "vitest run --config packages/core/vitest.config.integration.ts packages/core/tests/integration/critical-illness-claim/workflows/claim-processing.test.ts",
    "test:integration:critical-illness-structured": "vitest run --config packages/core/vitest.config.integration.ts packages/core/tests/integration/critical-illness-claim-structured/workflows/claim-processing.test.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "npm run test:watch -w @agent-system/core",
    "lint": "eslint packages",
    "lint:fix": "eslint packages --fix",
    "format": "prettier --write packages",
    "format:check": "prettier --check packages",
    "check": "npm run lint && npm run format:check && npm run test",
    "example:configuration": "tsx packages/core/examples/configuration.ts",
    "example:critical-illness": "tsx packages/core/examples/critical-illness-claim.ts",
    "example:default-agent": "tsx packages/core/examples/default-agent.ts",
    "example:inline-agents": "tsx packages/core/examples/inline-agents.ts",
    "example:logging": "tsx packages/core/examples/logging.ts",
    "example:mcp": "tsx packages/core/examples/mcp-integration.ts",
    "example:orchestration": "tsx packages/core/examples/orchestration.ts",
    "example:quickstart": "tsx packages/core/examples/quickstart.ts",
    "example:script-tools": "tsx packages/core/examples/script-tools.ts",
    "example:session-analyzer": "tsx packages/core/examples/session-analyzer.ts",
    "example:werewolf": "tsx packages/core/examples/werewolf-game.ts",
    "example:workflow-pipeline": "tsx packages/core/examples/workflow-pipeline.ts",
    "clean": "rm -rf packages/*/dist",
    "trace": "tsx packages/core/src/tracing/simple-tracer.ts",
    "trace:last": "tsx packages/core/src/tracing/simple-tracer.ts $(ls -t sessions/*.jsonl | head -1 | xargs basename .jsonl)"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@types/uuid": "^10.0.0",
    "ai": "^5.0.28",
    "dotenv": "^16.4.5",
    "gray-matter": "^4.0.3",
    "openai": "^4.29.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "tsx": "^4.7.1",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  }
}

{
  "// Note": "This is an example configuration. Agents are organized as follows:",
  "// Structure": {
    "agents/": "Template agents for library users",
    "examples/*/agents/": "Example-specific agents",
    "tests/integration/test-agents/": "Test-specific agents"
  },

  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=America/New_York"],
      "description": "Time and timezone utilities"
    }
  },

  "builtinTools": {
    "enabled": true,
    "tools": ["read", "write", "list", "task", "todowrite"]
  },

  "agents": {
    "directory": "./agents",
    "additionalDirectories": []
  },

  "execution": {
    "defaultModel": "claude-3-5-haiku-latest",
    "maxDepth": 10,
    "maxIterations": 100,
    "timeout": 300000
  }
}
